{
  "name": "vdtree",
  "version": "0.6.0",
  "description": "Write your components once; Use them in vanilla JS, React, Svelte, SSR and more",
  "module": "dist/vdtree.es.js",
  "main": "dist/vdtree.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "ssr": "ts-node src/main/main_ssr.tsx",
    "build": "vite build && tsc --emitDeclarationOnly",
    "declaration": "tsc --emitDeclarationOnly",
    "serve": "vite preview",
    "test": "ts-mocha -p tests/tsconfig.tests.json -r jsdom-global/register 'tests/**/*.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/express": "^4.17.12",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.9.0",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "chai": "^4.3.4",
    "express": "^4.17.1",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.5"
  },
  "dependencies": {
    "boost-web-core": "^1.12.0",
    "diff-dom": "^4.2.2"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "author": "Leone",
